{% block page_content_element_product_listing %}
	<div class="col-12 listing-wrapper">
		{% block page_content_element_product_listing_row %}
			<div class="row">
				{% block page_content_element_product_listing_col %}
					{% for product in element.data.listing.entities.elements %}
						<div class="listing-col col-sm-6 col-lg-3">
							{% block page_content_element_product_listing_box %}
								{% sw_include '@Storefront/component/product/widget/box.html.twig' with {layout: 'minimal'} %}
							{% endblock %}
						</div>
					{% endfor %}
				{% endblock %}
			</div>
		{% endblock %}
	</div>
{% endblock %}
